{
    "name": "eleventy-barebones",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all clean browser eleventy:dev",
        "build": "npm-run-all clean eleventy:prod",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist",
        "browser": "opener http://localhost:8000",
        "format": "npx prettier . --write",
        "format:check": "npx prettier . --check"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kyle-norris/eleventy-barebones.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "module",
    "bugs": {
        "url": "https://github.com/kyle-norris/eleventy-barebones/issues"
    },
    "homepage": "https://github.com/kyle-norris/eleventy-barebones#readme",
    "dependencies": {
        "@11ty/eleventy": "^3.1.2"
    },
    "devDependencies": {
        "cross-env": "^10.1.0",
        "del-cli": "^7.0.0",
        "esbuild": "^0.27.2",
        "npm-run-all": "^4.1.5",
        "opener": "^1.5.2",
        "prettier": "^3.8.0",
        "prettier-plugin-jinja-template": "^2.1.0",
        "sass": "^1.97.2"
    }
