{
    "name": "eleventy-barebones",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm run clean && npm-run-all -p js:dev eleventy:dev browser",
        "build": "npm-run-all -p lint clean && npm-run-all -p js:prod eleventy:prod",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist",
        "browser": "opener http://localhost:8000",
        "format": "npx prettier . --write",
        "lint": "npx eslint src/assets/js/**",
        "js:prod": "cross-env ELEVENTY_ENV=production node esbuild.js",
        "js:dev": "cross-env ELEVENTY_ENV=development node esbuild.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kyle-norris/eleventy-barebones.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "module",
    "bugs": {
        "url": "https://github.com/kyle-norris/eleventy-barebones/issues"
    },
    "homepage": "https://github.com/kyle-norris/eleventy-barebones#readme",
    "dependencies": {
        "@11ty/eleventy": "^3.1.2",
        "bootstrap": "^5.3.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "cross-env": "^10.1.0",
        "del-cli": "^7.0.0",
        "esbuild": "^0.27.2",
        "esbuild-sass-plugin": "^3.6.0",
        "eslint": "^9.39.2",
        "globals": "^17.0.0",
        "npm-run-all": "^4.1.5",
        "opener": "^1.5.2",
        "prettier": "^3.8.0",
        "prettier-plugin-jinja-template": "^2.1.0",
        "sass": "^1.97.2"
    }
}
